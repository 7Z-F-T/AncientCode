import java.io.*;
public class concat {
	public static void main(String[] args) throws FileNotFoundException,IOException{
		//definitions
		FileReader fr;
		FileWriter fw;
		BufferedReader br;
		BufferedWriter bw;
		
		fr=new FileReader(args[1]);//read from the second file
		fw=new FileWriter(args[0],true);//write to the first file
		
		br=new BufferedReader(fr);
		bw=new BufferedWriter(fw);
		
		String s="good";
		bw.write(s);
		
	}
		

}

