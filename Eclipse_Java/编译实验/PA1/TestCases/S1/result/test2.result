program
    class Main <empty>
        vardecl i inttype
        static func main voidtype
            formals
            stmtblock
                vardecl h classtype Hoo
                assign
                    varref i
                    intconst 4
                assign
                    varref h
                    newobj Goo
                call printNumbers
                    varref h
                    varref i
    class Hoo <empty>
        func printNumbers inttype
            formals
                vardecl n inttype
            stmtblock
                vardecl i inttype
                if
                    les
                        intconst 10
                        varref n
                    stmtblock
                        assign
                            varref n
                            intconst 10
                assign
                    varref i
                    intconst 0
                while
                    stmtblock
                        print
                            varref i
                        assign
                            varref i
                            mul
                                varref i
                                intconst 2
                print
                    stringconst "\n"
                return
                    varref i
    class Goo Hoo
        func printNumbers inttype
            formals
                vardecl n inttype
            stmtblock
                print
                    varref n
                return
                    intconst 1
